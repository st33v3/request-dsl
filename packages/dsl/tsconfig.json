{
    "compilerOptions": {
      "target": "ESNext",
      "allowJs": false,
      "skipLibCheck": false,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "resolveJsonModule": true,
      "declaration": true,
      "declarationMap": true,
      "outDir": "./dist",
      "declarationDir": "./dist",
      "emitDeclarationOnly": true
    },
    "include": ["src"],
  }
  